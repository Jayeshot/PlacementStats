<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.placementstats">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".HomeScreen.CourseContentPackage.AddQuestion"></activity>
        <activity android:name=".HomeScreen.ProgrammingSolutionWebView" />
        <activity android:name=".HomeScreen.CourseContentPackage.CourseContent" />
        <activity android:name=".HomeScreen.tempAddCourse" />
        <activity android:name=".HomeScreen.CourseDetail.CourseList" />
        <activity android:name=".HomeScreen.HomeActivity" />
        <activity android:name=".UserInfo" />
        <activity android:name=".otpVerification" />
        <activity
            android:name=".MainActivity"
            tools:ignore="Instantiatable">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>